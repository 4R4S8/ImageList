@model IEnumerable<ImageView>
  @{ ViewData["Title"] = "Index"; }

  <body>
  <div class="container p-3">
    <div class="row">
      <div class="col" >
        <ul class="list-group-item-primary ">
          @foreach (var obj in Model) { if (obj.Extension!="") {
          <ul>
            <li><a href="@obj.Path">@obj.Name</a></li>
          </ul>
          continue; }
          <li><a href="@obj.Path">@obj.Name</a></li>
          }
        </ul>
      </div>
      <div class="form-group border col ">
        <img
          id="manipulableImage"
          src="https://media.wired.com/photos/598e35994ab8482c0d6946e0/master/w_1920,c_limit/phonepicutres-TA.jpg"
          alt="Image place holder"
          class="img-fluid rounded float-right"
          style="display: none"
        />
         <canvas style="display: block;
    margin: 1em auto;
    background: #fff;
    border: 1px solid #ccc" class="rounded"></canvas>
      </div>
    </div>
  </div>
 
  <script src="~/js/site.js"></script>
</body>
